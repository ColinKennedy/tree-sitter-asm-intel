{
  "name": "asm_intel",
  "rules": {
    "module": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "instruction"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "instruction": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "mnemonic"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "operand"
            }
          }
        ]
      }
    },
    "mnemonic": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "operand": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        }
      ]
    },
    "register": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "other"
        }
      ]
    },
    "float": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "-?\\d*\\.\\d+(e[-+]?\\d+(\\.\\d*)?)?"
        },
        {
          "type": "PATTERN",
          "value": "-?\\d+\\.(\\d+)?(e[-+]?\\d+(\\.\\d*)?)?"
        }
      ]
    },
    "other": {
      "type": "STRING",
      "value": "thing"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

